{
	"name": "psycode",
	"displayName": "psycode",
	"description": "Psyche Language Server",
	"publisher": "nobita0918",
	"version": "0.1.0",
	"license": "MIT",
	"engines": {
		"vscode": "^1.45.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:psycode.helloWorld",
		"onCommand:psycode.generateFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "psycode.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "psycode.generateFile",
				"title": "Generate File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run build",
		"build": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"format": "yarn lint --fix",
		"watch": "yarn build --watch",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "7.1.1",
		"@types/mocha": "7.0.2",
		"@types/node": "13.13.9",
		"@types/vscode": "1.45.1",
		"@typescript-eslint/eslint-plugin": "2.34.0",
		"@typescript-eslint/parser": "2.34.0",
		"eslint": "6.8.0",
		"eslint-plugin-simple-import-sort": "5.0.3",
		"glob": "7.1.6",
		"mocha": "7.1.2",
		"typescript": "3.9.3",
		"vscode-test": "1.3.0"
	},
	"dependencies": {
		"dayjs": "^1.8.27"
	}
}
