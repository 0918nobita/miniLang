global sp
  i32.const 63999
endglobal

function push(value i32) void
  get_global sp
  get_local value
  i32.store
  get_global sp
  i32.const 4
  i32.sub
  set_global sp
endfunction

function pop i32
  get_global sp
  i32.const 4
  i32.add
  set_global sp
  get_global sp
  i32.load
endfunction

function top i32
  get_global sp
  i32.const 4
  i32.add
  i32.load
endfunction
